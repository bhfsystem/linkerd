#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  if [[ -f "$shome/vendor/linkerd/linkerd-0.5.0-exec" ]]; then
    if [[ -f "$shome/vendor/linkerd/namerdd-0.5.0-exec" ]]; then
      return 0
    fi
  fi

  return 1
}

source sub "$BASH_SOURCE" "$@"
