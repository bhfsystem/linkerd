#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  if [[ "$($shome/script/version | jq -r '.linkerd')" != "$LINKERD_VERSION" ]]; then
    return 1
  fi

  if [[ "$($shome/script/version | jq -r '.namerd')" != "$NAMERD_VERSION" ]]; then
    return 1
  fi

  return 0
}

source sub "$0" "$@"
